<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane fx:id="m_pane" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.gos.infinite_recharge.sd_widgets.super_structure.SuperStructureController">

    <center>
        <Group fx:id="m_group">
            <children>
            <Rectangle fx:id="m_robotBase" x="0" y="50" height="7.25" width="30.0" fill="lightskyblue"/>
            <Circle fx:id="m_robotShooter" radius="4" centerX="8" centerY="10" fill="plum"/>
            <Rectangle fx:id="m_robotConveyor" x="4" y="10" height="40.0" width="8.0" fill="coral"/>
            <Rectangle fx:id="m_robotIntake" x="-16" y="45" height="5" width="20.0" fill="seagreen"/>
            </children>
        </Group>
    </center>
</BorderPane>